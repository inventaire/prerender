{
  "author": "Prerender.io",
  "name": "prerender",
  "description": "Service to prerender Javascript rendered pages for SEO",
  "version": "3.22.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/prerender/prerender"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "phantom": "^0.7.2",
    "phantomjs": "^1.9.15",
    "prerender-level-cache": "0.2.0",
    "tree-kill": "^1.0.0"
  },
  "bin": {
    "prerender": "server.js"
  },
  "scripts": {
    "reset": "./scripts/reset",
    "archive-logs": "./scripts/archive_logs",
    "build": "coffee -c ./lib/plugins/*.coffee",
    "build-watch": "coffee -cw ./lib/plugins/*.coffee",
    "prestart": "npm run archive-logs",
    "start": "npm run build; supervisor -w reload.js server.js",
    "start-dev": "npm run build-watch & supervisor -w . server.js",
    "daemon-start": "npm run build; aeternum -o logs/server.log -e logs/error.log -- supervisor -w reload.js server.js",
    "reload": "touch reload.js"
  }
}
