{
  "author": "Prerender.io",
  "name": "prerender",
  "description": "Service to prerender Javascript rendered pages for SEO",
  "version": "3.19.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/prerender/prerender"
  },
  "dependencies": {
    "aws-sdk": "^2.1.8",
    "cache-manager": "^0.16.0",
    "lodash": "^3.1.0",
    "phantom": "^0.7.2",
    "phantomjs": "^1.9.15",
    "prerender-level-cache": "0.0.3",
    "tree-kill": "0.0.6"
  },
  "bin": {
    "prerender": "server.js"
  },
  "scripts": {
    "reset": "./scripts/reset",
    "archive-logs": "./scripts/archive_logs",
    "build": "coffee -c ./lib/plugins/rewriteUrl.coffee",
    "build-watch": "coffee -cw ./lib/plugins/rewriteUrl.coffee",
    "prestart": "npm run archive-logs",
    "start": "aeternum -o logs/server.log -e logs/error.log > run/prerender.pid -- supervisor server.js"
  }
}
